{
    "name": "digital-marketing-framework/typo3-distributor-json-request",
    "type": "typo3-cms-extension",
    "description": "Digital Marketing Framework - Distributor - Json Request",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "TYPO3",
        "extension",
        "form",
        "formrelay",
        "http",
        "request",
        "json"
    ],
    "homepage": "https://www.mediatis.de",
    "authors": [
        {
            "email": "info@mediatis.de",
            "homepage": "https://www.mediatis.de",
            "name": "Mediatis AG",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "typo3/cms-core": "^11.5 || ^12.4",
        "digital-marketing-framework/typo3-distributor-core": "dev-main",
        "digital-marketing-framework/distributor-json-request": "dev-main"
    },
    "require-dev": {
        "nimut/testing-framework": "^6.0",
        "typo3/coding-standards": "^0.5.2"
    },
    "autoload": {
        "psr-4": {
            "DigitalMarketingFramework\\Typo3\\Distributor\\JsonRequest\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigitalMarketingFramework\\Typo3\\Distributor\\JsonRequest\\Tests\\": "Tests"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true
        }
    },
    "scripts": {
        "link-extension": [
            "mkdir -p .Build/Web/typo3conf/ext/",
            "[ -L .Build/Web/typo3conf/ext/digitalmarketingframework_distributor_json_request ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/digitalmarketingframework_distributor_json_request"
        ]
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/Web",
            "extension-key": "digitalmarketingframework_distributor_json_request"
        }
    }
}
